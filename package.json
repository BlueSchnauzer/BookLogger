    "eslint-plugin-svelte": "^2.35.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vitest-dom": "^0.1.1"
  },
  "type": "module",
  "dependencies": {
    "chart.js": "^4.4.0",
    "googleapis": "^128.0.0",
    "mongodb": "^6.2.0",
    "mongodb-memory-server": "^9.0.1",
    "nprogress": "^0.2.0",
